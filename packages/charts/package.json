{
    "name": "react-stockcharts3",
    "version": "1.0.0",
    "description": "React charts specific to finance.",
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts",
    "files": [
        "lib",
        "src"
    ],
    "sideEffects": false,
    "author": "reactivemarkets",
    "keywords": [
        "charts",
        "charting",
        "stockcharts",
        "finance",
        "financial",
        "finance-chart",
        "react",
        "d3"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/fernesrou/react-stockcharts3.git"
    },
    "bugs": {
        "url": "https://github.com/fernesrou/react-stockcharts3/issues"
    },
    "scripts": {
        "build": "npm run clean && npm run compile",
        "clean": "rimraf lib",
        "compile": "tsc -p tsconfig.json"
    },
    "dependencies": {
        "@react-stockcharts3/annotations": "file:../annotations",
        "@react-stockcharts3/axes": "file:../axes",
        "@react-stockcharts3/coordinates": "file:../coordinates",
        "@react-stockcharts3/core": "file:../core",
        "@react-stockcharts3/indicators": "file:../indicators",
        "@react-stockcharts3/interactive": "file:../interactive",
        "@react-stockcharts3/scales": "file:../scales",
        "@react-stockcharts3/series": "file:../series",
        "@react-stockcharts3/tooltip": "file:../tooltip",
        "@react-stockcharts3/utils": "file:../utils"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    }
}
